{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "dist",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "declaration": true,
    "allowJs": false,
    "sourceMap": true,
    "inlineSources": true,
    "lib": [
      "es2017",
      "dom"
    ]
  },
  "include": [
    "lib/**/*"
  ]
}
